{
  "manifest_version": 2,

  "name": "Epitech-Planning",
  "description": "Cette extension permet de visualiser et d'exporter son planning",
  "version": "1.0",

  "browser_action" : {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": ["https://intra.epitech.eu/planning/load?format=json*", "storage"]
}